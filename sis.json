{
    "id": "/hrm-moe/sis-1",
    "acceptedResourceRoles": [
      "slave_public"
    ],
    "backoffFactor": 1.15,
    "backoffSeconds": 1,
    "container": {  
      "type": "DOCKER",
      "volumes": [],
      "docker": {
        "image": "moelk/openemis-nginx:latest",
        "portMappings": [
          { "hostPort": 0, "containerPort": 80, "servicePort": 10002 }
        ],
        "forcePullImage": true,
        "privileged": false,
        "parameters": []
      }
    },
    "labels":{
      "HAPROXY_0_VHOST":"sis.moe.gov.lk",
      "HAPROXY_GROUP":"external"
    },
    "cpus": 1,
    "disk": 0,
    "env": {
      "DB_HOST": "hrm-moesis-db.marathon.l4lb.thisdcos.directory:3306",
      "DB_USER": "openuser",
      "DB_PASSWORD": "openpass",
      "DB_NAME": "openemis"
    },
    "instances": 1,
    "maxLaunchDelaySeconds": 3600,
    "mem": 1024,
    "gpus": 0,
    "networks": [
      {
        "mode": "container/bridge"
      }
    ],
    "requirePorts": false,
    "upgradeStrategy": {
      "maximumOverCapacity": 1,
      "minimumHealthCapacity": 1
    },
    "killSelection": "YOUNGEST_FIRST",
    "unreachableStrategy": {
      "inactiveAfterSeconds": 0,
      "expungeAfterSeconds": 0
    },
    "healthChecks": [
        {
          "gracePeriodSeconds": 300,
          "intervalSeconds": 60,
          "maxConsecutiveFailures": 3,
          "portIndex": 0,
          "timeoutSeconds": 20,
          "delaySeconds": 15,
          "protocol": "TCP"
        }
      ],
    "fetch": [],
    "constraints": []
  }