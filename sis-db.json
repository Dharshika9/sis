{
  "id": "/hrm-moe/sis-db",
  "args": [
    "--datadir=/mnt/cluster_storage/hrmmoe/sisdb",
    "--max_connections= 1001",
    "--max_allowed_packet=1024M",
    "--sql_mode=NO_ZERO_IN_DATE,NO_ZERO_DATE,ERROR_FOR_DIVISION_BY_ZERO,NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION"
  ],
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 3306,
        "hostPort": 0,
        "labels": {
          "VIP_0": "/hrm-moe/sis-db:3306"
        },
        "protocol": "tcp",
        "name": "dbsismoe"
      }
    ],
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/mnt/cluster_storage/hrmmoe/sisdb",
        "hostPath": "mysqldata",
        "mode": "RW"
      },
      {
        "persistent": {
          "type": "root",
          "size": 1000000,
          "constraints": []
        },
        "mode": "RW",
        "containerPath": "mysqldata"
      }
    ],
    "docker": {
      "image": "mysql:5.7",
      "forcePullImage": false,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 2,
  "disk": 0,
  "env": {
    "MYSQL_ROOT_PASSWORD": "secret",
    "MYSQL_DATABASE": "openemis",
    "MYSQL_USER": "openeuser",
    "MYSQL_PASSWORD": "openepass"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "TCP"
    }
  ],
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": 8192,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "residency": {
    "relaunchEscalationTimeoutSeconds": 10,
    "taskLostBehavior": "WAIT_FOREVER"
  },
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": "disabled",
  "fetch": [],
  "constraints": []
}